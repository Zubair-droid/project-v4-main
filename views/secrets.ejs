<%- include('partials/header') %>

<div style="width: 100%;" >
  <div class = "jumbotron text-center"  style="background-color: burlywood;">
    <i style="padding: 10px;" class="fas fa-hand-holding-heart fa-10x"></i>
    <h1  style = "padding: 10px;" class="display-3">Your request has been added. Thank you!</h1>


    
     
        <%usersWithSecrets.forEach(function(user){ %>
         
        <div style="margin: 10px;padding: 10px;" class="card col-lg-4 col-md-6 col-sm-12">
          <div style="color:black; font-family:'Poppins',sans-serif"; class="container">     
          
            <h4 class="secret-text"><%=user.username%></h4>
            <h4 class="secret-text"><%=user.secret%></h4>
             <p class="secret-text"><%=user.contact%></p>
        
          </div>
        </div>   
          <% }) %>        
        
            
      



      
    
    
   
    


    <hr>
    <a style = "padding: 10px;" class="btn btn-light btn-lg" href="/logout" role="button">Log Out</a>
    <a style = "padding: 10px;" class="btn btn-dark btn-lg" href="/submit" role="button">Submit a request</a>
  </div>
</div>

